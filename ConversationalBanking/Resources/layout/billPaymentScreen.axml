<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/accountDetails_ParentLayout"
    p1:minWidth="25px"
    p1:minHeight="25px">
    <LinearLayout
        p1:orientation="vertical"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/linearLayout1"
        p1:background="@color/background">
        <android.support.v7.widget.Toolbar
            p1:layout_width="match_parent"
            p1:layout_height="wrap_content"
            p1:id="@+id/billPayment_Toolbar"
            p1:elevation="4dp"
            style="@style/ToolbarContent"
            app:navigationIcon="@drawable/ic_action_bill_payment"
            app:contentInsetStartWithNavigation="16dp"
            p1:theme="@style/ToolbarContentWithoutBG" />
        <RelativeLayout
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="match_parent"
            p1:visibility="gone"
            p1:id="@+id/bill_presentLayout">
            <ScrollView
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="fill_parent"
                p1:id="@+id/scrollView1">
                <LinearLayout
                    p1:orientation="vertical"
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="match_parent"
                    p1:layout_height="match_parent"
                    p1:id="@+id/linearLayout3">
                    <android.support.v7.widget.CardView
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="fill_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/cardView1"
                        p1:layout_alignParentTop="true"
                        p1:layout_below="@id/linearLayout1"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        p1:clipToPadding="false"
                        app:cardUseCompatPadding="false"
                        p1:elevation="4dp"
                        p1:layout_marginBottom="8dp"
                        p1:layout_marginTop="16dp"
                        p1:layout_marginLeft="16dp"
                        p1:layout_marginRight="16dp">
                        <LinearLayout
                            p1:orientation="vertical"
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="match_parent"
                            p1:layout_height="match_parent"
                            p1:id="@+id/linearLayout2"
                            p1:paddingTop="20dp">
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout4"
                                p1:weightSum="1"
                                p1:layout_marginBottom="16dp"
                                p1:layout_marginLeft="20dp"
                                p1:layout_marginRight="20dp">
                                <TextView
                                    p1:text="Electricity Bill Details"
                                    p1:id="@+id/textView2"
                                    style="@style/textBoldBlue"
                                    p1:singleLine="true"
                                    p1:gravity="left"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent" />
                                <LinearLayout
                                    p1:orientation="vertical"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout5"
                                    p1:minWidth="25px"
                                    p1:minHeight="25px"
                                    p1:gravity="center_vertical|right">
                                    <ImageView
                                        p1:src="@drawable/bseslogo"
                                        p1:id="@+id/imageView3"
                                        p1:layout_width="84.5dp"
                                        p1:layout_height="17.5dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:minWidth="25px"
                                p1:minHeight="25px"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout6"
                                p1:layout_marginBottom="4dp"
                                p1:layout_marginRight="20dp"
                                p1:layout_marginLeft="20dp">
                                <TextView
                                    p1:text="Payable Amount: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView3"
                                    style="@style/helpingText"
                                    p1:singleLine="true"
                                    p1:gravity="center_vertical|left" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout7"
                                    p1:layout_weight="0.7"
                                    p1:gravity="center_vertical|right">
                                    <ImageView
                                        p1:src="@drawable/currency_sign"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/imageView2"
                                        p1:layout_marginRight="4dp"
                                        style="@style/blueTint" />
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_Amount"
                                        p1:layout_marginLeft="4dp"
                                        p1:singleLine="true"
                                        style="@style/availableBalance"
                                        p1:gravity="center_vertical"
                                        p1:textSize="20sp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp"
                                p1:layout_marginLeft="20dp"
                                p1:layout_marginRight="20dp">
                                <TextView
                                    p1:text="Billing Date: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <TextView
                                        p1:text="default_Month"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_BillingMonth"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp"
                                p1:layout_marginRight="20dp"
                                p1:layout_marginLeft="20dp">
                                <TextView
                                    p1:text="CA Number: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_CANumber"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp"
                                p1:layout_marginLeft="20dp"
                                p1:layout_marginRight="20dp">
                                <TextView
                                    p1:text="Meter Reading: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_MeterReading"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp"
                                p1:layout_marginLeft="20dp"
                                p1:layout_marginRight="20dp">
                                <TextView
                                    p1:text="Meter Number: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_MeterNumber"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="vertical"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout4"
                                p1:weightSum="1"
                                p1:background="@color/dueDate"
                                p1:paddingLeft="20dp"
                                p1:paddingRight="20dp"
                                p1:paddingTop="8dp"
                                p1:paddingBottom="8dp"
                                p1:layout_marginTop="8dp">
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout5"
                                    p1:gravity="center_vertical"
                                    p1:layout_weight="1">
                                    <TextView
                                        p1:text="Due Date"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/textView1"
                                        style="@style/helpingText"
                                        p1:gravity="center_vertical"
                                        p1:layout_marginRight="2dp" />
                                    <TextView
                                        p1:layout_width="match_parent"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_DueDate"
                                        style="@style/availableBalance"
                                        p1:gravity="center_vertical|right"
                                        p1:text="@string/default_date"
                                        p1:layout_marginLeft="2dp"
                                        p1:textSize="20sp" />
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="fill_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/cardView1"
                        p1:layout_alignParentTop="true"
                        p1:layout_below="@id/linearLayout1"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        p1:clipToPadding="false"
                        app:cardUseCompatPadding="false"
                        p1:elevation="4dp"
                        p1:layout_marginBottom="8dp"
                        p1:layout_marginTop="8dp"
                        p1:layout_marginLeft="16dp"
                        p1:layout_marginRight="16dp">
                        <LinearLayout
                            p1:orientation="vertical"
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="match_parent"
                            p1:layout_height="match_parent"
                            p1:id="@+id/linearLayout2"
                            p1:padding="20dp">
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout4"
                                p1:weightSum="1"
                                p1:layout_marginBottom="16dp">
                                <TextView
                                    p1:text="Your Account Overview"
                                    p1:id="@+id/textView2"
                                    style="@style/textBoldBlue"
                                    p1:singleLine="true"
                                    p1:gravity="left"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:layout_weight="0.95" />
                                <ImageView
                                    p1:src="@drawable/bank"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="wrap_content"
                                    p1:id="@+id/imageView2"
                                    p1:tint="@color/primary_dark"
                                    p1:layout_gravity="right"
                                    p1:layout_weight="0.05" />
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp">
                                <TextView
                                    p1:text="Available Balance: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <ImageView
                                        p1:src="@drawable/currency_sign"
                                        p1:layout_width="16dp"
                                        p1:layout_height="16dp"
                                        p1:id="@+id/imageView1"
                                        p1:layout_marginRight="2dp"
                                        style="@style/blueTint"
                                        p1:layout_gravity="center_vertical" />
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/billPayment_SelfBalance"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="16dp">
                                <TextView
                                    p1:text="Account Number: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.1"
                                    style="@style/helpingText" />
                                <TextView
                                    p1:text="@string/default_accNumber"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/billPayment_SelfAccountNumber"
                                    p1:layout_weight="0.9"
                                    style="@style/availableBalance"
                                    p1:singleLine="true"
                                    p1:gravity="right" />
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                </LinearLayout>
            </ScrollView>
            <LinearLayout
                p1:orientation="horizontal"
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/linearLayout2"
                p1:layout_alignParentBottom="true"
                p1:layout_above="@id/scrollView1"
                p1:weightSum="1">
                <Button
                    p1:text="BACK"
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/billPayment_Cancel"
                    p1:layout_weight="0.3"
                    p1:layout_marginRight="4dp"
                    p1:background="@drawable/roundbutton_layout2"
                    p1:elevation="4dp"
                    p1:textColor="@color/primary_dark"
                    p1:layout_marginLeft="16dp"
                    p1:layout_marginBottom="16dp"
                    p1:layout_marginTop="16dp" />
                <Button
                    p1:text="MAKE PAYMENT"
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/billPayment_Approve"
                    p1:layout_weight="0.7"
                    p1:background="@drawable/roundbutton_green"
                    style="@style/roundedButton"
                    p1:elevation="4dp"
                    p1:layout_marginLeft="4dp"
                    p1:layout_marginRight="16dp"
                    p1:layout_marginBottom="16dp"
                    p1:layout_marginTop="16dp" />
            </LinearLayout>
            <LinearLayout
                p1:orientation="vertical"
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:id="@+id/linearLayout8"
                p1:layout_width="wrap_content"
                p1:layout_height="wrap_content" />
        </RelativeLayout>
        <RelativeLayout
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="match_parent"
            p1:id="@+id/bill_notPresentLayout"
            p1:visibility="visible">
            <Button
                p1:text="BACK TO DASHBOARD"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/billPayment_BackDashboard"
                p1:elevation="4dp"
                p1:background="@drawable/roundbutton_green"
                p1:layout_alignParentBottom="true"
                p1:layout_marginBottom="16dp"
                p1:layout_marginLeft="16dp"
                p1:layout_marginRight="16dp"
                p1:layout_marginTop="16dp"
                style="@style/roundedButton" />
            <TextView
                p1:text="No Active Bill Found!"
                p1:id="@+id/textView4"
                style="@style/serviceCardText"
                p1:textColor="@color/primary_dark"
                p1:textSize="16sp"
                p1:layout_width="match_parent"
                p1:layout_height="fill_parent"
                p1:layout_above="@id/billPayment_BackDashboard"
                p1:gravity="center"
                p1:layout_centerInParent="true" />
        </RelativeLayout>
    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>