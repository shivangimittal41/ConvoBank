<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/coordinatorLayout1">
    <LinearLayout
        p1:orientation="vertical"
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/linearLayout1"
        p1:background="@color/background">
        <android.support.v7.widget.Toolbar
            p1:layout_width="match_parent"
            p1:layout_height="wrap_content"
            p1:id="@+id/previousTransactions_Toolbar"
            p1:elevation="4dp"
            app:navigationIcon="@drawable/ic_action_prev_bill"
            app:contentInsetStartWithNavigation="16dp"
            style="@style/ToolbarContent"
            p1:theme="@style/ToolbarContentWithoutBG" />
        <LinearLayout
            p1:orientation="vertical"
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="fill_parent"
            p1:layout_height="fill_parent"
            p1:id="@+id/linearLayout2">
            <android.support.v7.widget.RecyclerView
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="fill_parent"
                p1:layout_height="fill_parent"
                p1:id="@+id/previousTransaction_RecyclerView"
                p1:paddingTop="4dp" />
        </LinearLayout>
    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>