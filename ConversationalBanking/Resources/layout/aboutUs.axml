<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/fundTransfer_ParentLayout"
    p1:minWidth="25px"
    p1:minHeight="25px">
    <LinearLayout
        p1:orientation="vertical"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/linearLayout1">
        <android.support.v7.widget.Toolbar
            p1:layout_width="match_parent"
            p1:layout_height="wrap_content"
            p1:id="@+id/fundTransfer_Toolbar"
            p1:elevation="4dp"
            app:contentInsetStartWithNavigation="16dp"
            style="@style/ToolbarContent"
            p1:theme="@style/ToolbarContentWithoutBG"
            app:navigationIcon="@drawable/account_circle" />
        <RelativeLayout
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="fill_parent"
            p1:id="@+id/relativeLayout1"
            p1:background="@color/background"
            p1:paddingLeft="12dp"
            p1:paddingRight="12dp"
            p1:paddingTop="20dp"
            p1:focusableInTouchMode="true">
            <LinearLayout
                p1:orientation="horizontal"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/linearLayout1"
                p1:layout_alignParentBottom="true"
                p1:weightSum="1"
                p1:paddingLeft="4dp"
                p1:paddingRight="4dp"
                p1:clipToPadding="false"
                p1:clipChildren="false"
                p1:paddingBottom="20dp">
                <Button
                    p1:text="@string/button_cancel"
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/servicefundTransfer_Cancel"
                    p1:layout_weight="0.3"
                    p1:layout_marginRight="4dp"
                    p1:background="@drawable/roundbutton_layout2"
                    p1:elevation="4dp"
                    p1:textColor="@color/primary_dark" />
                <Button
                    p1:text="@string/button_transfer"
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/servicefundTransfer_Approve"
                    p1:layout_weight="0.7"
                    p1:background="@drawable/roundbutton_green"
                    style="@style/roundedButton"
                    p1:elevation="4dp"
                    p1:layout_marginLeft="4dp" />
            </LinearLayout>
            <ScrollView
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/scrollView"
                p1:layout_alignParentTop="true"
                p1:layout_below="@id/linearLayout1">
                <LinearLayout
                    p1:orientation="vertical"
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/linearLayout4"
                    p1:layout_alignParentTop="true"
                    p1:layout_above="@id/linearLayout1">
                    <android.support.v7.widget.CardView
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="fill_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/cardView1"
                        p1:layout_alignParentTop="true"
                        p1:layout_below="@id/linearLayout1"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        p1:clipToPadding="false"
                        app:cardUseCompatPadding="false"
                        p1:elevation="4dp"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="16dp">
                        <LinearLayout
                            p1:orientation="vertical"
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="match_parent"
                            p1:layout_height="match_parent"
                            p1:id="@+id/linearLayout2"
                            p1:padding="20dp"
                            p1:focusable="auto">
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout4"
                                p1:weightSum="1"
                                p1:layout_marginBottom="16dp">
                                <TextView
                                    p1:text="Fund Transfer Details"
                                    p1:id="@+id/textView2"
                                    style="@style/textBoldBlue"
                                    p1:singleLine="true"
                                    p1:gravity="left"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:layout_weight="0.95" />
                                <ImageView
                                    p1:src="@drawable/information_outline"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="wrap_content"
                                    p1:id="@+id/imageView2"
                                    p1:tint="@color/primary_dark"
                                    p1:layout_gravity="right"
                                    p1:layout_weight="0.05" />
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="vertical"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="8dp"
                                p1:layout_marginTop="4dp">
                                <TextView
                                    p1:text="Beneficiary Account Number"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.1"
                                    style="@style/helpingText" />
                                <EditText
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/fundTransfer_RecieverAccount"
                                    p1:background="@drawable/mockup2_textfieldlowerborder"
                                    p1:textColor="@color/editTextColor"
                                    p1:textColorHint="@color/primary_light"
                                    p1:hint="Account Number"
                                    p1:maxLength="12"
                                    p1:inputType="text"
                                    p1:paddingTop="4dp"
                                    p1:paddingLeft="2dp"
                                    p1:paddingRight="2dp" />
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="vertical"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="8dp"
                                p1:layout_marginTop="4dp">
                                <TextView
                                    p1:text="Amount to be Transferred"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.1"
                                    style="@style/helpingText"
                                    p1:layout_marginBottom="2dp" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="wrap_content"
                                    p1:id="@+id/linearLayout5"
                                    p1:weightSum="1"
                                    p1:paddingTop="4dp">
                                    <ImageView
                                        p1:src="@drawable/currency_sign"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="wrap_content"
                                        p1:id="@+id/imageView1"
                                        p1:layout_marginRight="2dp"
                                        style="@style/blueTint"
                                        p1:layout_gravity="center_vertical"
                                        p1:layout_weight="0.1" />
                                    <EditText
                                        p1:id="@+id/fundTransfer_RecieverAmount"
                                        p1:textColor="@color/editTextColor"
                                        p1:textColorHint="@color/primary_light"
                                        p1:hint="Amount to be Transferred"
                                        p1:paddingTop="4dp"
                                        p1:layout_width="match_parent"
                                        p1:layout_height="match_parent"
                                        p1:layout_weight="0.9"
                                        p1:layout_marginLeft="4dp"
                                        p1:paddingLeft="2dp"
                                        p1:inputType="number"
                                        p1:background="@drawable/mockup2_textfieldlowerborder"
                                        p1:paddingRight="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="vertical"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="8dp"
                                p1:layout_marginTop="4dp">
                                <TextView
                                    p1:text="Additional Comments"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.1"
                                    style="@style/helpingText" />
                                <EditText
                                    p1:inputType="textMultiLine"
                                    p1:layout_width="match_parent"
                                    p1:layout_height="wrap_content"
                                    p1:id="@+id/fundTransfer_RecieverRemarks"
                                    p1:background="@drawable/mockup2_textfieldlowerborder"
                                    p1:hint="Additional Comments"
                                    p1:paddingLeft="2dp"
                                    p1:paddingTop="4dp"
                                    p1:textColor="@color/editTextColor"
                                    p1:textColorHint="@color/primary_light"
                                    p1:paddingRight="2dp" />
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="fill_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/cardView1"
                        p1:layout_alignParentTop="true"
                        p1:layout_below="@id/linearLayout1"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        p1:clipToPadding="false"
                        app:cardUseCompatPadding="false"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:elevation="4dp"
                        p1:layout_marginBottom="8dp">
                        <LinearLayout
                            p1:orientation="vertical"
                            p1:minWidth="25px"
                            p1:minHeight="25px"
                            p1:layout_width="match_parent"
                            p1:layout_height="match_parent"
                            p1:id="@+id/linearLayout2"
                            p1:padding="20dp">
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout4"
                                p1:weightSum="1"
                                p1:layout_marginBottom="16dp">
                                <TextView
                                    p1:text="Your Account Overview"
                                    p1:id="@+id/textView2"
                                    style="@style/textBoldBlue"
                                    p1:singleLine="true"
                                    p1:gravity="left"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:layout_weight="0.95" />
                                <ImageView
                                    p1:src="@drawable/bank"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="wrap_content"
                                    p1:id="@+id/imageView2"
                                    p1:tint="@color/primary_dark"
                                    p1:layout_gravity="right"
                                    p1:layout_weight="0.05" />
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="4dp">
                                <TextView
                                    p1:text="Available Balance: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.4"
                                    style="@style/helpingText" />
                                <LinearLayout
                                    p1:orientation="horizontal"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/linearLayout2"
                                    p1:layout_weight="0.6"
                                    p1:gravity="bottom|right">
                                    <ImageView
                                        p1:src="@drawable/currency_sign"
                                        p1:layout_width="16dp"
                                        p1:layout_height="16dp"
                                        p1:id="@+id/imageView1"
                                        p1:layout_marginRight="2dp"
                                        style="@style/blueTint"
                                        p1:layout_gravity="center_vertical" />
                                    <TextView
                                        p1:text="@string/default_balance"
                                        p1:layout_width="wrap_content"
                                        p1:layout_height="match_parent"
                                        p1:id="@+id/fundTransfer_SelfBalance"
                                        p1:gravity="right"
                                        style="@style/availableBalance"
                                        p1:layout_marginLeft="2dp" />
                                </LinearLayout>
                            </LinearLayout>
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="match_parent"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/linearLayout3"
                                p1:weightSum="1"
                                p1:gravity="center_vertical"
                                p1:layout_marginBottom="16dp">
                                <TextView
                                    p1:text="Account Number: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    p1:layout_weight="0.1"
                                    style="@style/helpingText" />
                                <TextView
                                    p1:text="@string/default_accNumber"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/fundTransfer_SelfAccountNumber"
                                    p1:layout_weight="0.9"
                                    style="@style/availableBalance"
                                    p1:singleLine="true"
                                    p1:gravity="right" />
                            </LinearLayout>
                        </LinearLayout>
                    </android.support.v7.widget.CardView>
                </LinearLayout>
            </ScrollView>
        </RelativeLayout>
    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>