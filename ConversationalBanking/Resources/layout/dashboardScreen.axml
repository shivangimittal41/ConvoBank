<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/dashboard_ParentLayout"
    p1:minWidth="25px"
    p1:minHeight="25px">
    <RelativeLayout
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/relativeLayout1"
        p1:padding="12dp">
        <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="wrap_content"
            p1:id="@+id/cardView1"
            p1:layout_alignParentBottom="true"
            p1:clipToPadding="false"
            p1:clipChildren="false"
            p1:elevation="2dp"
            app:cardCornerRadius="4dp"
            app:cardElevation="2dp"
            p1:layout_marginRight="4dp"
            p1:layout_marginLeft="4dp"
            p1:layout_marginTop="4dp"
            p1:layout_marginBottom="4dp"
            app:cardUseCompatPadding="true">
            <LinearLayout
                p1:orientation="horizontal"
                p1:layout_width="match_parent"
                p1:layout_height="match_parent"
                p1:id="@+id/linearLayout1"
                p1:gravity="center_vertical"
                p1:padding="16dp"
                p1:weightSum="1">
                <TextView
                    p1:text="Available Balance: "
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/textView1"
                    style="@style/helpingText"
                    p1:layout_weight="0.4"
                    p1:gravity="center_vertical" />
                <LinearLayout
                    p1:orientation="horizontal"
                    p1:layout_width="wrap_content"
                    p1:layout_height="match_parent"
                    p1:id="@+id/linearLayout2"
                    p1:layout_weight="0.6"
                    p1:gravity="right">
                    <ImageView
                        p1:src="@drawable/currency_sign"
                        p1:layout_width="wrap_content"
                        p1:layout_height="match_parent"
                        p1:id="@+id/imageView1"
                        p1:layout_marginRight="2dp"
                        style="@style/blueTint" />
                    <TextView
                        p1:text="@string/default_balance"
                        p1:layout_width="wrap_content"
                        p1:layout_height="match_parent"
                        p1:id="@+id/dashboard_availableBalance"
                        p1:gravity="center_vertical|right"
                        style="@style/availableBalance"
                        p1:layout_marginLeft="2dp" />
                </LinearLayout>
            </LinearLayout>
        </android.support.v7.widget.CardView>
        <ScrollView
            p1:layout_height="match_parent"
            p1:layout_width="match_parent"
            p1:layout_alignParentTop="true"
            p1:layout_below="@id/cardView1">
            <LinearLayout
                p1:orientation="vertical"
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="match_parent"
                p1:id="@+id/linearLayout3">
                <LinearLayout
                    p1:orientation="horizontal"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/linearLayout4"
                    p1:weightSum="1">
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        app:cardUseCompatPadding="true"
                        p1:id="@+id/mainService_FundTransfer"
                        p1:clickable="true"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp"
                        p1:hapticFeedbackEnabled="true"
                        p1:paddingRight="2dp">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                p1:src="@drawable/fundtransfer_banner"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed" />
                        </android.support.constraint.ConstraintLayout>
                        <TextView
                            p1:text="@string/mainServicesFundTransfer"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/textView3"
                            p1:layout_gravity="bottom"
                            style="@style/serviceCardText"
                            p1:gravity="center|left"
                            p1:background="@color/serviceCardBGText"
                            p1:textColor="@color/secondary_text"
                            p1:textSize="16sp"
                            p1:textStyle="normal"
                            p1:singleLine="true"
                            p1:padding="8dp" />
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        app:cardUseCompatPadding="true"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp"
                        p1:id="@+id/mainService_BillPayment"
                        p1:clickable="true"
                        p1:paddingLeft="2dp">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                p1:src="@drawable/billpayment_banner"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed" />
                        </android.support.constraint.ConstraintLayout>
                        <TextView
                            p1:text="@string/mainServicesBillPayment"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/textView3"
                            p1:layout_gravity="bottom"
                            style="@style/serviceCardText"
                            p1:gravity="center|left"
                            p1:padding="8dp"
                            p1:background="@color/serviceCardBGText"
                            p1:textColor="@color/secondary_text"
                            p1:textSize="16sp"
                            p1:textStyle="normal"
                            p1:singleLine="true" />
                    </android.support.v7.widget.CardView>
                </LinearLayout>
                <LinearLayout
                    p1:orientation="horizontal"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/linearLayout4"
                    p1:weightSum="1">
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp"
                        app:cardUseCompatPadding="true"
                        p1:id="@+id/mainService_PrevTransaction"
                        p1:clickable="true">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                p1:src="@drawable/prevbill_banner"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed" />
                        </android.support.constraint.ConstraintLayout>
                        <TextView
                            p1:text="@string/mainServicesPrevTransactions"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/textView3"
                            p1:layout_gravity="bottom"
                            style="@style/serviceCardText"
                            p1:gravity="center|left"
                            p1:padding="8dp"
                            p1:background="@color/serviceCardBGText"
                            p1:textColor="@color/secondary_text"
                            p1:textSize="16sp"
                            p1:textStyle="normal"
                            p1:singleLine="true" />
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp"
                        app:cardElevation="4dp"
                        app:cardUseCompatPadding="true"
                        p1:id="@+id/mainService_AccountDetails"
                        p1:clickable="true">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                p1:src="@drawable/accountdetails_banner"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed" />
                        </android.support.constraint.ConstraintLayout>
                        <TextView
                            p1:text="@string/mainServicesAccountDetails"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/textView3"
                            p1:layout_gravity="bottom"
                            style="@style/serviceCardText"
                            p1:gravity="center|left"
                            p1:padding="8dp"
                            p1:background="@color/serviceCardBGText"
                            p1:textColor="@color/secondary_text"
                            p1:textSize="16sp"
                            p1:textStyle="normal"
                            p1:singleLine="true" />
                    </android.support.v7.widget.CardView>
                </LinearLayout>
                <LinearLayout
                    p1:orientation="horizontal"
                    p1:layout_width="match_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/linearLayout4"
                    p1:weightSum="1">
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        app:cardUseCompatPadding="true"
                        p1:background="@color/background"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp"
                        p1:id="@+id/mainService_NewServices">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed"
                                app:backgroundTint="#ffe7e7e7"
                                p1:padding="56dp"
                                p1:src="@drawable/add"
                                p1:tint="@color/primary_dark"
                                p1:background="@color/background"
                                p1:backgroundTint="@color/background"
                                app:backgroundTintMode="src_over" />
                        </android.support.constraint.ConstraintLayout>
                    </android.support.v7.widget.CardView>
                    <android.support.v7.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
                        style="@style/CardView.Light"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:layout_weight="0.5"
                        p1:clipToPadding="false"
                        p1:clipChildren="false"
                        p1:elevation="4dp"
                        app:cardCornerRadius="4dp"
                        app:cardElevation="4dp"
                        app:cardUseCompatPadding="true"
                        p1:id="@+id/cardView2"
                        p1:visibility="invisible">
                        <android.support.constraint.ConstraintLayout
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/constraintLayout1">
                            <ImageView
                                p1:id="@+id/media_image"
                                p1:layout_width="0dp"
                                p1:layout_height="0dp"
                                p1:scaleType="centerCrop"
                                p1:tint="@color/primary_dark"
                                p1:src="@drawable/bg1_placeholder"
                                app:layout_constraintDimensionRatio="H,1:1"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintBottom_toTopOf="@+id/share_button"
                                app:layout_constraintVertical_chainStyle="packed" />
                        </android.support.constraint.ConstraintLayout>
                        <TextView
                            p1:text="@string/mainServicesBillPayment"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/textView3"
                            p1:layout_gravity="bottom"
                            style="@style/serviceCardText"
                            p1:gravity="center|left"
                            p1:padding="8dp"
                            p1:background="#8c000000"
                            p1:textColor="@color/secondary_text"
                            p1:textSize="16sp"
                            p1:textStyle="normal"
                            p1:singleLine="true" />
                    </android.support.v7.widget.CardView>
                </LinearLayout>
            </LinearLayout>
        </ScrollView>
    </RelativeLayout>
</android.support.design.widget.CoordinatorLayout>