<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:p1="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    p1:minWidth="25px"
    p1:minHeight="25px"
    p1:layout_width="match_parent"
    p1:layout_height="match_parent"
    p1:id="@+id/fundTransferConfirmation_ParentLayout">
    <LinearLayout
        p1:orientation="vertical"
        p1:minWidth="25px"
        p1:minHeight="25px"
        p1:layout_width="match_parent"
        p1:layout_height="match_parent"
        p1:id="@+id/linearLayout1"
        p1:background="@color/background">
        <android.support.v7.widget.Toolbar
            p1:layout_width="match_parent"
            p1:layout_height="wrap_content"
            p1:id="@+id/fundTransferConfirmation_Toolbar"
            p1:elevation="4dp"
            style="@style/ToolbarContent"
            app:navigationIcon="@drawable/ic_action_fund_transfer"
            app:contentInsetStartWithNavigation="16dp"
            p1:theme="@style/ToolbarContentWithoutBG" />
        <LinearLayout
            p1:orientation="vertical"
            p1:minWidth="25px"
            p1:minHeight="25px"
            p1:layout_width="match_parent"
            p1:layout_height="match_parent"
            p1:id="@+id/linearLayout2"
            p1:paddingLeft="12dp"
            p1:paddingRight="12dp"
            p1:paddingTop="12dp">
            <android.support.v7.widget.CardView
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="wrap_content"
                p1:id="@+id/cardView1"
                app:cardCornerRadius="4dp"
                app:cardElevation="4dp"
                p1:layout_marginRight="4dp"
                p1:layout_marginLeft="4dp"
                p1:layout_marginTop="4dp"
                p1:layout_marginBottom="4dp"
                app:cardUseCompatPadding="false"
                p1:clipToPadding="false">
                <LinearLayout
                    p1:orientation="vertical"
                    p1:layout_width="match_parent"
                    p1:layout_height="match_parent"
                    p1:id="@+id/linearLayout3">
                    <LinearLayout
                        p1:orientation="vertical"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/linearLayout4"
                        p1:weightSum="1"
                        p1:background="@color/titleLight"
                        p1:paddingLeft="20dp"
                        p1:paddingRight="20dp"
                        p1:paddingTop="8dp"
                        p1:paddingBottom="8dp"
                        p1:layout_marginBottom="12dp">
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/linearLayout4"
                            p1:weightSum="1">
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/linearLayout5"
                                p1:layout_weight="0.95"
                                p1:gravity="center_vertical">
                                <TextView
                                    p1:text="Transaction ID: "
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/textView1"
                                    style="@style/helpingText"
                                    p1:gravity="center_vertical" />
                                <TextView
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/fundTransferConf_txnID"
                                    style="@style/availableBalance"
                                    p1:gravity="center_vertical"
                                    p1:text="@string/default_TxnId" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:id="@+id/linearLayout5"
                            p1:gravity="center_vertical"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content">
                            <TextView
                                p1:text="Transaction Date: "
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/textView1"
                                style="@style/helpingText"
                                p1:gravity="center_vertical" />
                            <TextView
                                p1:text="@string/default_date"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/fundTransferConf_Date"
                                style="@style/availableBalance"
                                p1:layout_weight="0.3"
                                p1:singleLine="true"
                                p1:gravity="center_vertical"
                                p1:layout_marginLeft="2dp" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        p1:orientation="horizontal"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/linearLayout6"
                        p1:weightSum="1"
                        p1:layout_marginBottom="8dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginLeft="20dp"
                        p1:layout_marginRight="20dp">
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/linearLayout7"
                            p1:layout_weight="1"
                            p1:gravity="center_vertical">
                            <ImageView
                                p1:src="@drawable/currency_sign"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/imageView2"
                                p1:layout_marginRight="4dp"
                                style="@style/blueTint" />
                            <TextView
                                p1:text="@string/default_balance"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/fundTransferConf_Amount"
                                p1:layout_marginLeft="4dp"
                                p1:singleLine="true"
                                p1:textSize="20sp"
                                style="@style/availableBalance"
                                p1:gravity="center_vertical" />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        p1:orientation="horizontal"
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/linearLayout8"
                        p1:layout_marginBottom="4dp"
                        p1:gravity="center_vertical"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginLeft="20dp"
                        p1:layout_marginRight="20dp">
                        <TextView
                            p1:text="has been Transferred to"
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/textView5"
                            style="@style/helpingText"
                            p1:gravity="center_vertical" />
                    </LinearLayout>
                    <LinearLayout
                        p1:orientation="horizontal"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/linearLayout3"
                        p1:weightSum="1"
                        p1:gravity="center_vertical"
                        p1:layout_marginBottom="12dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginLeft="20dp"
                        p1:layout_marginRight="20dp">
                        <TextView
                            p1:text="Account Number "
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/textView1"
                            p1:layout_weight="0.1"
                            style="@style/helpingText"
                            p1:gravity="left|center_vertical" />
                        <TextView
                            p1:text="@string/default_accNumber"
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/fundTransferConf_AccNumber"
                            p1:layout_weight="0.9"
                            style="@style/availableBalance"
                            p1:singleLine="true"
                            p1:gravity="right|center_vertical" />
                    </LinearLayout>
                    <LinearLayout
                        p1:orientation="vertical"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/linearLayout3"
                        p1:weightSum="1"
                        p1:gravity="center_vertical"
                        p1:layout_marginBottom="12dp"
                        p1:layout_marginLeft="20dp"
                        p1:layout_marginRight="20dp">
                        <TextView
                            p1:text="Remarks"
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/textView1"
                            p1:layout_weight="0.1"
                            style="@style/helpingText"
                            p1:gravity="left|center_vertical" />
                        <TextView
                            p1:text="@string/default_Remarks"
                            p1:layout_width="wrap_content"
                            p1:layout_height="match_parent"
                            p1:id="@+id/fundTransferConf_Remark"
                            p1:layout_weight="0.9"
                            style="@style/availableBalance"
                            p1:singleLine="false"
                            p1:gravity="left" />
                    </LinearLayout>
                </LinearLayout>
            </android.support.v7.widget.CardView>
            <LinearLayout
                p1:orientation="vertical"
                p1:minWidth="25px"
                p1:minHeight="25px"
                p1:layout_width="match_parent"
                p1:layout_height="match_parent"
                p1:id="@+id/linearLayout10"
                p1:gravity="top"
                p1:layout_marginTop="4dp"
                p1:paddingBottom="12dp"
                p1:clipToPadding="false">
                <android.support.v7.widget.CardView
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="fill_parent"
                    p1:layout_height="wrap_content"
                    p1:id="@+id/cardView1"
                    p1:layout_alignParentTop="true"
                    p1:layout_below="@id/linearLayout1"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    p1:clipToPadding="false"
                    app:cardUseCompatPadding="false"
                    p1:elevation="4dp"
                    p1:layout_marginBottom="8dp"
                    p1:layout_marginTop="4dp"
                    p1:layout_marginLeft="4dp"
                    p1:layout_marginRight="4dp">
                    <LinearLayout
                        p1:orientation="vertical"
                        p1:minWidth="25px"
                        p1:minHeight="25px"
                        p1:layout_width="match_parent"
                        p1:layout_height="match_parent"
                        p1:id="@+id/linearLayout2"
                        p1:padding="20dp">
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/linearLayout4"
                            p1:weightSum="1"
                            p1:layout_marginBottom="16dp">
                            <TextView
                                p1:text="Your Account Overview"
                                p1:id="@+id/textView2"
                                style="@style/textBoldBlue"
                                p1:singleLine="true"
                                p1:gravity="left"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:layout_weight="0.95" />
                            <ImageView
                                p1:src="@drawable/bank"
                                p1:layout_width="wrap_content"
                                p1:layout_height="wrap_content"
                                p1:id="@+id/imageView2"
                                p1:tint="@color/primary_dark"
                                p1:layout_gravity="right"
                                p1:layout_weight="0.05" />
                        </LinearLayout>
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/linearLayout3"
                            p1:weightSum="1"
                            p1:gravity="center_vertical"
                            p1:layout_marginBottom="4dp">
                            <TextView
                                p1:text="Available Balance: "
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/textView1"
                                p1:layout_weight="0.4"
                                style="@style/helpingText" />
                            <LinearLayout
                                p1:orientation="horizontal"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/linearLayout2"
                                p1:layout_weight="0.6"
                                p1:gravity="bottom|right">
                                <ImageView
                                    p1:src="@drawable/currency_sign"
                                    p1:layout_width="16dp"
                                    p1:layout_height="16dp"
                                    p1:id="@+id/imageView1"
                                    p1:layout_marginRight="2dp"
                                    style="@style/blueTint"
                                    p1:layout_gravity="center_vertical" />
                                <TextView
                                    p1:text="@string/default_balance"
                                    p1:layout_width="wrap_content"
                                    p1:layout_height="match_parent"
                                    p1:id="@+id/fundTransferConfirmation_SelfBalance"
                                    p1:gravity="right"
                                    style="@style/availableBalance"
                                    p1:layout_marginLeft="2dp" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            p1:orientation="horizontal"
                            p1:layout_width="match_parent"
                            p1:layout_height="wrap_content"
                            p1:id="@+id/linearLayout3"
                            p1:weightSum="1"
                            p1:gravity="center_vertical"
                            p1:layout_marginBottom="16dp">
                            <TextView
                                p1:text="Account Number: "
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/textView1"
                                p1:layout_weight="0.1"
                                style="@style/helpingText" />
                            <TextView
                                p1:text="@string/default_accNumber"
                                p1:layout_width="wrap_content"
                                p1:layout_height="match_parent"
                                p1:id="@+id/fundTransferConfirmation_SelfAccountNumber"
                                p1:layout_weight="0.9"
                                style="@style/availableBalance"
                                p1:singleLine="true"
                                p1:gravity="right" />
                        </LinearLayout>
                    </LinearLayout>
                </android.support.v7.widget.CardView>
                <LinearLayout
                    p1:orientation="vertical"
                    p1:minWidth="25px"
                    p1:minHeight="25px"
                    p1:layout_width="match_parent"
                    p1:layout_height="match_parent"
                    p1:id="@+id/linearLayout11"
                    p1:gravity="bottom">
                    <Button
                        p1:text="@string/button_backDashboard"
                        p1:layout_width="match_parent"
                        p1:layout_height="wrap_content"
                        p1:id="@+id/fundTransferConf_MainDashboard"
                        p1:background="@drawable/roundbutton_green"
                        style="@style/roundedButton"
                        p1:elevation="4dp"
                        p1:layout_marginLeft="4dp"
                        p1:layout_marginRight="4dp"
                        p1:layout_marginTop="4dp"
                        p1:layout_marginBottom="4dp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>